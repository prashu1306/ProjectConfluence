<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.innoapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <supports-screens
        android:largeScreens="true"
        android:normalScreens="true"
        android:smallScreens="true"
        android:xlargeScreens="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity_details"
            android:label="@string/title_activity_details"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.user.innoapp.MainActivity" />
        </activity>
        <activity
            android:name=".ProshowPage"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.user.innoapp.MainActivity" />
        </activity>
        <activity
            android:name=".ProshowDetails"
            android:parentActivityName=".ProshowPage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.user.innoapp.ProshowPage" />
        </activity>
        <activity
            android:name=".ImageOnClick"
            android:parentActivityName=".ProshowDetails">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.user.innoapp.ProshowDetails" />
        </activity>

        <activity android:name=".FaqandHelp"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.user.innoapp.MainActivity" />
        </activity>
        <activity android:name=".Accessibility"
            android:parentActivityName=".FaqandHelp">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.user.innoapp.FaqandHelp"/>
        </activity>
        <activity android:name=".Hospitality"
            android:parentActivityName=".FaqandHelp">
            <meta-data
                android:name="android.support.PARENTA_ACTIVITY"
                android:value="com.example.user.innoapp.FaqandHelp"/>
        </activity>
        <activity android:name=".FAQs"
            android:parentActivityName=".FaqandHelp">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.user.innoapp.FaqandHelp" />
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".EventImage"></activity>
    </application>

</manifest>